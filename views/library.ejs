<html>

<head>
  <% include partials/head.ejs %>
</head>

<body class="homepage">
  <div id="page-wrapper">

    <!-- Header -->
    <% include partials/menu.ejs %>

      <!-- Main -->
      <div id="main-wrapper">
        <div class="container">
          <div class="row 200%">
            <div class="4u 12u$(medium)">
              <div id="sidebar">

                <!-- Sidebar -->
                <% include partials/sidebar-one.ejs %>
                  <% include partials/sidebar-two.ejs %>


              </div>
            </div>
            <div class="8u 12u$(medium) important(medium)">
              <div id="content">

                <!-- Content -->
                <article>

                  <h2><%= pageTitle %></h2>
                  <p>
                    <% if (myData.length > 0) { %>
                      <table>
                        <% for (i=0; i<myData.length; i++) { %>
                          <tr>
                            <% var item = myData[i]; %>
                              <td>
                                <%= item.data.title %>
                              </td>
                              <td>
                                <a href="<%= item.data.url %>" target="_blank">link</a>
                              </td>
                          </tr>
                          <% } %>
                      </table>
                      <% } %>
                        <!-- <ul>
                      <% if (myData.length > 0) { %>
                        <%  for (i=0;i < myData.length;i++) { %>
                          <li>
                            <% var item = myData[i]; %>
                              <%= item.data.title %>
                          </li>
                          <% } %>
                            <% } %>
                    </ul> -->
                  </p>

                </article>

              </div>
            </div>
          </div>
        </div>
      </div>

      <!-- Footer -->
      <% include partials/footer.ejs %>

        <!-- Scripts -->
        <% include partials/scripts.ejs %>

</body>

</html>
